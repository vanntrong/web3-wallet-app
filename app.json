{
  "expo": {
    "name": "Web3 Wallet App",
    "slug": "web3-wallet-app",
    "scheme": "web3-wallet-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "splash": {
      "image": "./assets/logo-splash-screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      },
      "jsEngine": "hermes",
      "infoPlist": {
        "NSFaceIDUsageDescription": "Allow ${PRODUCT_NAME} to use Face ID"
      },
      "bundleIdentifier": "com.vovantrong.web3walletapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#62A2F8"
      },
      "package": "com.vovantrong.web3walletapp",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.USE_BIOMETRIC"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-secure-store",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to access your Face ID biometric data."
        }
      ],
      "expo-camera",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to use Face ID."
        }
      ],
      ["expo-notifications"],
      "expo-router",
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access camera."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "84d6a575-907c-4796-85d9-cd4530f3e6ac"
      }
    }
  }
}
